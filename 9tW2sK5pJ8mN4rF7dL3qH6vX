   end
   get()

wait(1)

--check for 2nd load
local function createfake()
local plrrr = game:GetService("Players").LocalPlayer
local charr = plrrr.Character
local hrpp = charr:FindFirstChild("HumanoidRootPart")
if hrpp:FindFirstChild("Nil") then
	local playerok = game:GetService("Players").LocalPlayer
	playerok:Kick("spam execution detected")
else
	local fld = Instance.new("Folder")
	fld.Parent = hrpp
	fld.Name = ("Nil")
	return true
end
end

createfake()

local YOUR_USER_ID = 9048872413
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- Check if YOU are in the server (as any player)
local ownerIsInServer = false
for _, player in ipairs(Players:GetPlayers()) do
    if player.UserId == YOUR_USER_ID then
        ownerIsInServer = true
        break
    end
end

-- If owner is in server, but THIS client is NOT the owner → kick self
if ownerIsInServer and LocalPlayer.UserId ~= YOUR_USER_ID then
    -- 💥 Kick the executor (not the owner)
    LocalPlayer:Kick("hiii")
    return -- stop script execution
end

-- ✅ If we reach here:
--   - Either owner is NOT in server, OR
--   - This IS the owner → allow script to run
local function startfreecam()
	local camera = game.Workspace.CurrentCamera
	camera.CameraType = Enum.CameraType.Fixed
end
local function stopfreecam()
	local camera = game.Workspace.CurrentCamera
	camera.CameraType = Enum.CameraType.Custom
end

